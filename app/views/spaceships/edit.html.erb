<%= form_with model: @spaceship do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :payload %>
  <%= f.number_field :payload %>
  <%= f.label :pax %>
  <%= f.number_field :pax %>
  <%= f.label :availability %>
  <%= f.text_field :availability %>
  <%= f.submit %>
<% end %>

<%= link_to "Abort!", spaceships_path(@spaceship)%>
